                            </td>
                            <td>
                                <%= rand(date_to_set.to_time..(date_to_set.to_datetime + 1.day).to_time).strftime('%A, %d %b %Y %l:%M %p')%>
                            </td>
                            
                            <td>
                                
                            </td>
                        </tr>
                    <% end %>
                    
                </tbody>
              </table>
          </div>
        </div>
      </div>
    </div>
  </div>
 </section>
</div>

<div class="modal fade hide" id="modal-default" aria-hidden="true">
  <div class="modal-dialog modal-lg modal-dialog-scrollable">
    <div class="modal-content">
      <div class="modal-body">
        <div class="card " id="data-to-append">
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary close-modal pull-right">Close</button>
      </div>
    </div>
  </div>
</div>
<div class="modal-backdrop fade hide" style="display: none"></div>


<script>
    $(function(){
        $(".modal-header .close, .modal-footer .close-modal").unbind('click').bind('click', function(){
            hideModal()
        })
        $(".provider-name-td").unbind('click').bind('click', function(e){
            e.preventDefault();
            console.log("clicked")
            let $tr= $(this).closest('tr')
            let data = JSON.parse($tr.attr('data-service-provider'))
            let doctor = JSON.parse($tr.attr('data-doctor'))
            let members = JSON.parse($tr.attr('data-members'))
            let services = JSON.parse($tr.attr('data-sevice-no'))
            let mainService= data['Service']
            let all_categories= [{"category":"Housing","subcategory":"Housing Transition/ Navigation Services"},{"category":"Housing","subcategory":"Community Transition Services"},{"category":"Housing","subcategory":"Home Modifications"},{"category":"Personal Care","subcategory":"Personal Care and Homemaker Services"},{"category":"Personal Care","subcategory":"Sobering Centers"},{"category":"Food","subcategory":"Medically Tailored Meals"},{"category":"Training","subcategory":"Training"},{"category":"Transportation","subcategory":"Transportation"}]

            subservices = all_categories.filter((item)=> item['category'] === mainService).map((item)=> item['subcategory'])

            let names = "<%= (1..10).map { |x| fake_user_name }.join(', ') %>".split(', ');
            let random_dates = "<%= (1..10).map { |x| rand(45.days.ago..Time.now).strftime('%d %b %Y') }.join(', ') %>".split(', ');

            let doctorName =doctor['Name']
            let providerAddress =data['Address']
            let providerContact =formattedNumber(data['Phone'])

            let $content = $(`<div class="card-body box-profile"><div class="text-center"><img class="profile-user-img img-fluid img-circle" src="/assets/public-service.png" alt="User profile picture"></div><h3 class="profile-username text-center" id="provider-name">${doctorName}</h3><p class="text-muted text-center" id="provider-address">${providerAddress}</p><p class="text-muted text-center" id="provider-contact">${providerContact}</p><table class="table table-striped table-valign-middle" style="height: 200px; overflow-y: auto"><thead><tr><th>Member Name</th><th>Services Date</th><th>Service</th><th>Service Subcategory</th><th>Comments</th></tr></thead><tbody></tbody></table></div`)
            
            for (let index = 0; index < services; index++) {
                console.log("111")
                name = names[Math.floor(Math.random() * names.length)]
                service = mainService
                date = random_dates[Math.floor(Math.random() * random_dates.length)]
                subService = subservices[Math.floor(Math.random() * subservices.length)]
                let tr = `<tr><td>${name}</td><td>${date}</td><td>${service}</td><td>${subService}</td><td></td></tr>`
               $content.find('tbody').append(tr)
            }
            $("#data-to-append").html($content)
            showModal()
           
        })

        function showModal(){
          $("#modal-default, .modal-backdrop").removeClass('hide').addClass('show')
           .attr('role', 'dialog')
           .css('display', 'block')
        }
        function hideModal(){
            $("#data-to-append").html('')
           $("#modal-default, .modal-backdrop").addClass('hide').removeClass('show')
           .removeAttr('role')
           .css('display', 'none')
           
        }

        function formattedNumber(number){
            a = ''+number
            a = [a.slice(0, 7), "-", a.slice(7)].join('');
            a = [a.slice(0, 4), "-", a.slice(4)].join('');
            a = [a.slice(0, 4), ")", a.slice(4)].join('');
            a = [a.slice(0, 1), "(", a.slice(1)].join('');
            return a
        }
    })
</script>




