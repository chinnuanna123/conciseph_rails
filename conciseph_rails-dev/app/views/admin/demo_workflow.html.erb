            $(`.col-sm-6[data-type=${value}]`).removeClass('hide')
        })
        $(".by-dropdown").unbind('change').bind('change', function(){
            let value = $(this).val()
            console.log(value)
            $('.col-sm-6.by').addClass('hide')
            $(`.col-sm-6[data-type=${value}]`).removeClass('hide')
        })

        $(".artifact-selection").unbind('change').bind('change', function(){
            let value = $(this).val();
            if (value === 'File'){
               $(this).closest('tr').find('.artifact-textarea').addClass('hide')
               $(this).closest('tr').find('input[type="file"]').removeClass('hide')
            }else{
               $(this).closest('tr').find('.artifact-textarea').removeClass('hide')
               $(this).closest('tr').find('input[type="file"]').addClass('hide')
            }
        })

        $(".next-button").unbind('click').bind('click', function(){
         console.log("click")
            var nextTarget = $(this).attr('data-next-target')
            $(`.step`).removeClass('active')
            $(`.step[data-target='#${nextTarget}']`).addClass('active')

            $(".content.active.dstepper-block").removeClass('active dstepper-block')
            $(`#${nextTarget}`).addClass('active dstepper-block')
        })
    })

    $(document).ready( function () {
      $('.datatable').DataTable(
         {
         paging: false,
         info: false,
         searching: false,
         "scrollX": true,
         "ordering": false
         }
      );
   } );
</script>