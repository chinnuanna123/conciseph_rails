<!--<h1>Goals</h1> -->
 <div class="content-wrapper">
 <%=render 'header'%>
   
   
    <section class="content">
      <div class="container-fluid">
        <div class="row d-flex justify-content-center">
          <div class="col-lg-4 col-6">
            <div class="small-box bg-warning">
              <div class="inner">
                <h3><%=Overseer.count%></h3>
                <p>Count</p>
              </div>
              <div class="icon">
                <i class="ion ion-bag"></i>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-6">
           <a href="<%=new_overseer_path%>">
           <div class="small-box bg-primary">
              <div class="inner px-1">
                <h3>&nbsp;</h3>
                <p><%=CREATE_NEW_TILE_NAMES[params[:controller].to_sym]%></p>
              </div>
              <div class="icon">
                <i class="ion ion-pie-graph"></i>
              </div>
            </div>
           </a>
            
          </div>
        </div>
        <div class="row">
         <div class="col-lg-12">
           <div class="card">
            
             <div class="card-body table-responsive p-0">
                 <table class="table table-striped table-valign-middle">
                   <thead>
                      <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Created At</th>
                        <th colspan="3"></th>
                      </tr>
                    </thead>
                   <tbody>                 
                     <%= render  'listing', overseers: @overseers%>
                   </tbody>
                 </table>
             </div>
           </div>
         </div>
       </div>
     </div>
    </section>
   </div>

