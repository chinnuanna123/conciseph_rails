<!--<h1>Goals</h1> -->
 <div class="content-wrapper">
 <%=render 'shared/campaigns/header'%>
   
   
    <section class="content">
      <div class="container-fluid">
        <div class="row d-flex justify-content-center">
          <div class="col-lg-4 col-6">
            <div class="small-box bg-warning">
              <div class="inner">
                <h3><%=@api_logs.count%></h3>
                <p>Count</p>
              </div>
              <div class="icon">
                <i class="ion ion-bag"></i>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
         <div class="col-lg-12">
           <div class="card">
            
             <div class="card-body table-responsive p-0">
                 <table class="table table-striped table-valign-middle">
                   <thead>
                      <tr>
                        <th>url</th>
                        <th>payload</th>
                        <th>response_code</th>
                        <th>response_body</th>
                        <th>request_ip</th>
                        <th>Created At</th>
                        <th colspan="3"></th>
                      </tr>
                    </thead>
                   <tbody>                 
                     <%= render  'listing', overseers: @overseers%>
                   </tbody>
                 </table>
             </div>
           </div>
         </div>
       </div>
     </div>
    </section>
   </div>

